<!DOCTYPE html>
<html>
<head>
	<title></title>
	<style>
	</style>
</head>
<body>
<script type="text/javascript">
	// biến sout chứa thuộc tính kích cỡ bảng
	var sout = "<table border ='1' width ='300' cellspacing ='0' cellpadding ='5'>";
	//vòng lặp để thiết kế bảng.
	for (y =1;y <=10;y++){
		//thêm hàng vào bảng
			sout +="<tr>";
		for (x = 1; x <=10; x++){
			//thêm cột vào bảng tương ứng với mỗi hàng
			sout +='<td>' + x*y+'</td>';
		}
		sout +='</tr>';
	}
	sout+= '</table>';
	document.write(sout);

</script>

</body>
</html>
